{
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "env": {
      "browser": true,
      "node": true,
      "jasmine": true
  },
  "rules": {
      "max-len": [1, 120, 2, {"ignoreComments": true}],
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "no-prototype-builtins": 0,
      "import/prefer-default-export": 0,
      "no-console": 0,
      "no-use-before-define": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "no-unused-vars": 0,
      "no-nested-ternary": 0,
      "no-useless-escape": 0,
      "func-names": 0,
      "consistent-return": 0
  },
  "extends": ["airbnb-base"]
}
